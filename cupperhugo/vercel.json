{
    "version": 2,
    "builds": [
      {
        "src": "cupperhugo/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.11.2" }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static",
        "config":{"disDir":"staticfiles_build"}
      }
    ],
    "routes": [
      {
        "src": "/staticfiles/(.*)",
        "dest": "/staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "cupperhuho/wsgi.py"
      }
    ]
  }